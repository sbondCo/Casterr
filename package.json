{
  "name": "casterr",
  "version": "1.0.0",
  "private": true,
  "description": "Easy screen recording and sharing",
  "author": {
    "name": "sbondCo",
    "url": "https://sbond.co"
  },
  "main": "./entry/out/background.js",
  "scripts": {
    "serve": "npm run build:entry-scripts && node ./entry/out/serve.js",
    "build:entry-scripts": "tsc --project tsconfig.entry.json",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "prettier:formatall": "prettier --write '**/*.{ts,js,tsx,json}'",
    "prettier:check": "prettier --check '**/*.{ts,js,tsx,json}'",
    "lint": "eslint .",
    "build": "npm run build:entry-scripts && vite build && electron-builder build --config builder.config.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "nouislider": "^15.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.14",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "postcss": "^8.4.20",
    "prettier": "^2.8.0",
    "sass": "^1.55.0",
    "tailwindcss": "^3.1.2",
    "typescript": "^4.9.3",
    "vite": "^3.0.3",
    "vite-plugin-commonjs-externals": "^0.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "GPL-3.0"
}
