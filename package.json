{
  "name": "casterr",
  "version": "0.1.8-alpha",
  "private": true,
  "description": "Easy screen recording and sharing",
  "author": {
    "name": "sbondCo",
    "url": "https://sbond.co"
  },
  "main": "./entry/out/background.js",
  "scripts": {
    "serve": "tsc ./entry/background.ts ./entry/preload.ts ./entry/serve.ts --removeComments true --strict true --esModuleInterop true --outDir ./entry/out && node ./entry/out/serve.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "prettier:formatall": "prettier --write '**/*.{ts,js,tsx,json}'",
    "prettier:check": "prettier --check '**/*.{ts,js,tsx,json}'"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.3",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.1",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.3.7",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "electron-devtools-installer": "^3.2.0",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.4.4",
    "vite": "^2.6.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "GPL-3.0"
}
